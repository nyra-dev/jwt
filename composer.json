{
  "name": "nyra/jwt",
  "description": "JSON Web Token (JWT) library for PHP",
  "keywords": [
    "jwt"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "Nyra\\Jwt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nyra\\Jwt\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "René Preuß",
      "email": "rene@preuss.io"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "@php vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
